                <svg width="60px" height="60px" viewBox="0 0 40 40">
                  <filter id="specular" primitiveUnits="objectBoundingBox"> 
                    <feSpecularLighting
                      result="specOut" 
                      in="SourceGraphic" 
                      specularExponent="30" 
                      specularConstant="0.4" 
                      surfaceScale="1" 
                      lighting-color="#bbbbbb">
                      <fePointLight x="0.5" y="0.5" z="0.3" />
                    </feSpecularLighting>
                    <feComposite
                      in="SourceGraphic"
                      in2="specOut"
                      operator="arithmetic"
                      k1="0"
                      k2="1"
                      k3="1"
                      k4="0" />
                  </filter>
                  <image href="/svgs/heart-pulse-fill.svg" width="40" height="40" style="filter:url(#specular)">
                    <animate attributeName="height" from="35" to="40" dur="1.5s" begin="0s" repeatCount="indefinite"/>
                  </image>
                </svg>
                <hr class="hr">
