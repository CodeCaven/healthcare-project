    <div class="col-lg-9">
      <table data-page-length='10' class="table table-info table-bordered table-sm text-center shadow-5" id="table-met">
        <thead class="table-dark">
          <tr>
            <th data-field="activity-light">
                Light Activities <i class="fas fa-heart-pulse me-3" style="color:salmon"></i>
            </th>
            <th data-field="activity-moderate">
                Moderate Activities <i class="fas fa-heart-pulse me-3" style="color:salmon"></i> 
            </th>
            <th data-field="activity-vigorous">
                Vigorous Activities <i class="fas fa-heart-pulse me-3" style="color:salmon"></i>
            </th>
          </tr>
        </thead>
        <tbody id="table-met-body" style="cursor:pointer;">

        </tbody>
